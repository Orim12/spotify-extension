{
  "name": "spotify-controller",
  "displayName": "spotify controller",
  "description": "controller for spotify",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.108.1"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "spotify-controller.play",
        "title": "Spotify: Play"
      },
      {
        "command": "spotify-controller.pause",
        "title": "Spotify: Pause"
      },
      {
        "command": "spotify-controller.next",
        "title": "Spotify: Next Track"
      },
      {
        "command": "spotify-controller.previous",
        "title": "Spotify: Previous Track"
      },
      {
        "command": "spotify-controller.nowPlaying",
        "title": "Spotify: Show Now Playing"
      },
      {
        "command": "spotify-controller.setToken",
        "title": "Spotify: Set Access Token"
      }
    ],
    "configuration": {
      "title": "Spotify Controller",
      "properties": {
        "spotify-controller.accessToken": {
          "type": "string",
          "description": "Spotify API Access Token"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile && yarn run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.108.1",
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "typescript-eslint": "^8.52.0",
    "eslint": "^9.39.2",
    "typescript": "^5.9.3",
    "@vscode/test-cli": "^0.0.12",
    "@vscode/test-electron": "^2.5.2"
  },
  "dependencies": {
    "axios": "^1.6.0"
  }
}
